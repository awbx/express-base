{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true,
        "jest":true
    },
    "extends": ["airbnb-base", "prettier", "plugin:prettier/recommended"],
    "plugins": ["prettier","jest"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console":"off",
        "no-unused-vars": ["warn", { "argsIgnorePattern": "next" }],
        "global-require":"off"
    }
}
